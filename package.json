{
	"name": "dothttp-code",
	"displayName": "dothttp-code",
	"description": "run dothttp files in vscode natively",
	"version": "0.0.5",
	"publisher": "shivaprasanth",
	"repository": {
		"url": "https://github.com/cedric05/dothttp-runner"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:dothttp-vscode",
		"workspaceContains:**/*.http",
		"onView:dothttpEnvView",
		"onCommand:dothttp.command.run"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "dothttp-vscode",
				"aliases": [
					"dothttp-vscode"
				],
				"extensions": [
					".http",
					".dhttp"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "jsonc",
				"extensions": [
					".dothttp.json"
				],
				"filenames": [
					".dothttp.json"
				],
				"filenamePatterns": [
					"*.dothttp.json"
				]
			}
		],
		"grammars": [
			{
				"language": "dothttp-vscode",
				"scopeName": "source.dothttp-vscode",
				"path": "./syntaxes/grammer.json"
			}
		],
		"commands": [
			{
				"command": "dothttp.command.run",
				"title": "run first target",
				"icon": {
					"dark": "./media/rocket.svg",
					"light": "./media/rocket.svg"
				}
			},
			{
				"command": "dothttp.command.gencurl",
				"title": "generate curl for first target",
				"icon": {
					"dark": "media/mirror.svg",
					"light": "media/mirror.svg"
				}
			},
			{
				"command": "dothttp.command.toggle.experimental",
				"title": "toggles experimental setting",
				"category": "Dothttp",
				"icon": {
					"dark": "./media/gear.svg",
					"light": "./media/gear.svg"
				}
			},
			{
				"command": "dothttp.command.toggle.showheaders",
				"title": "toggles experimental setting",
				"category": "Dothttp",
				"icon": {
					"dark": "./media/gear.svg",
					"light": "./media/gear.svg"
				}
			},
			{
				"command": "dothttp.command.toggle.history",
				"title": "toggles history setting",
				"category": "Dothttp",
				"icon": {
					"dark": "./media/gear.svg",
					"light": "./media/gear.svg"
				}
			},
			{
				"command": "dothttp.command.toggle.nocookie",
				"title": "toggles cookie setting",
				"category": "Dothttp",
				"icon": {
					"dark": "./media/gear.svg",
					"light": "./media/gear.svg"
				}
			},
			{
				"command": "dothttp.command.import.external",
				"title": "import resource to http",
				"category": "Dothttp",
				"icon": {
					"dark": "./media/cloud-download.svg",
					"light": "./media/cloud-download.svg"
				}
			},
			{
				"command": "dothttpEnvView.refresh",
				"title": "Refresh",
				"icon": {
					"dark": "media/refresh.svg",
					"light": "media/refresh.svg"
				}
			},
			{
				"command": "dothttpEnvView.enableenv",
				"title": "enable env",
				"icon": {
					"dark": "./media/add.svg",
					"light": "./media/add.svg"
				}
			},
			{
				"command": "dothttpEnvView.disableenv",
				"title": "disable env",
				"icon": {
					"dark": "media/remove.svg",
					"light": "media/remove.svg"
				}
			},
			{
				"command": "dothttpPropView.add",
				"title": "add property",
				"icon": {
					"dark": "media/new-file.svg",
					"light": "media/new-file.svg"
				}
			},
			{
				"command": "dothttpPropView.enableproperty",
				"title": "enable property",
				"icon": {
					"dark": "./media/add.svg",
					"light": "./media/add.svg"
				}
			},
			{
				"command": "dothttpPropView.disableproperty",
				"title": "disable property",
				"icon": {
					"dark": "media/remove.svg",
					"light": "media/remove.svg"
				}
			},
			{
				"command": "dothttpPropView.copyPropertyValue",
				"title": "copy property value",
				"icon": {
					"dark": "./media/chrome-restore.svg",
					"light": "./media/chrome-restore.svg"
				}
			},
			{
				"command": "dothttpPropView.updateproperty",
				"title": "update property",
				"icon": {
					"dark": "./media/edit.svg",
					"light": "./media/edit.svg"
				}
			},
			{
				"command": "dothttpPropView.disableAllProperties",
				"title": "disable all properties",
				"icon": {
					"dark": "./media/close-all.svg",
					"light": "./media/close-all.svg"
				}
			},
			{
				"command": "dothttpPropView.removeproperty",
				"title": "remove property",
				"icon": {
					"dark": "./media/trash.svg",
					"light": "./media/trash.svg"
				}
			},
			{
				"command": "dothttpEnvView.opendothttpjson",
				"title": "open",
				"icon": {
					"dark": "./media/edit.svg",
					"light": "./media/edit.svg"
				}
			},
			{
				"command": "dothttpEnvView.copyPropertyValue",
				"title": "copy env property",
				"icon": {
					"dark": "./media/chrome-restore.svg",
					"light": "./media/chrome-restore.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "dothttp.command.run",
					"group": "navigation",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				},
				{
					"command": "dothttp.command.gencurl",
					"group": "navigation",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				}
			],
			"commandPalette": [
				{
					"command": "dothttp.command.run",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				},
				{
					"command": "dothttp.command.toggle.experimental",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				},
				{
					"command": "dothttp.command.toggle.nocookie",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				},
				{
					"command": "dothttp.command.toggle.history",
					"when": "resourceExtname == .http || resourceExtname == .dhttp"
				}
			],
			"view/title": [
				{
					"command": "dothttpEnvView.refresh",
					"when": "view == dothttpEnvView",
					"group": "navigation"
				},
				{
					"command": "dothttpEnvView.opendothttpjson",
					"when": "view == dothttpEnvView",
					"group": "navigation"
				},
				{
					"command": "dothttpPropView.add",
					"when": "view == dothttpPropView",
					"group": "navigation"
				},
				{
					"command": "dothttpPropView.disableAllProperties",
					"when": "view == dothttpPropView",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "dothttpEnvView.enableenv",
					"when": "view == dothttpEnvView && viewItem == environment",
					"group": "inline"
				},
				{
					"command": "dothttpEnvView.disableenv",
					"when": "view == dothttpEnvView && viewItem == enabledEnvironment",
					"group": "inline"
				},
				{
					"command": "dothttpEnvView.copyPropertyValue",
					"when": "view == dothttpEnvView && viewItem == property",
					"group": "inline"
				},
				{
					"command": "dothttpPropView.enableproperty",
					"when": "view == dothttpPropView && viewItem == disabled",
					"group": "inline"
				},
				{
					"command": "dothttpPropView.disableproperty",
					"when": "view == dothttpPropView && viewItem == enabled",
					"group": "inline"
				},
				{
					"command": "dothttpPropView.copyPropertyValue",
					"when": "view == dothttpPropView",
					"group": "inline"
				},
				{
					"command": "dothttpPropView.updateproperty",
					"when": "view == dothttpPropView",
					"group": "inline"
				},
				{
					"command": "dothttpPropView.removeproperty",
					"when": "view == dothttpPropView",
					"group": "inline"
				}
			]
		},
		"configuration": [
			{
				"title": "Dothttp Configuration",
				"properties": {
					"dothttp.conf.pythonpath": {
						"type": "string",
						"default": "/usr/bin/python3",
						"description": "python installation location for making requests"
					},
					"dothttp.conf.path": {
						"type": "string",
						"default": "/usr/bin/dothttp",
						"description": "dothttp installation location"
					},
					"dothttp.conf.experimental": {
						"type": "boolean",
						"default": false,
						"description": "enables experimental features by setting to `ture`"
					},
					"dothttp.conf.nocookie": {
						"type": "boolean",
						"default": false,
						"description": "disables cookie save feature by setting to `ture`"
					},
					"dothttp.conf.history": {
						"type": "boolean",
						"default": false,
						"description": "save history of requests by setting to `ture`"
					},
					"dothttp.conf.curl": {
						"type": "boolean",
						"default": false,
						"description": "genererate curl command only!"
					},
					"dothttp.conf.showheaders": {
						"type": "boolean",
						"default": false,
						"description": "show headers in seperate view"
					}
				}
			}
		],
		"views": {
			"explorer": [
				{
					"id": "dothttpEnvView",
					"name": "Dothttp env",
					"when": "dothttpEnvViewEnabled"
				},
				{
					"id": "dothttpPropView",
					"name": "Dothttp prop",
					"when": "dothttpPropViewEnabled"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/dateformat": "^3.0.1",
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.168",
		"@types/mime-types": "^2.1.0",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.20.0",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"dateformat": "^4.5.1",
		"jsonc-parser": "^3.0.0",
		"lodash": "^4.17.20",
		"mime-types": "^2.1.29"
	}
}